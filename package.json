{
  "name": "testing-react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist && mkdir dist",
    "build": "npm run clean && browserify -t [ babelify --presets [ es2015 react ] ] ./src/index.js > ./dist/bundle.js",
    "watch": "npm run clean && watchify --poll -v -t [ babelify --presets [ es2015 react ] ] ./src/index.js -o ./lib/bundle.js",
    "test": "babel-node test/test.* | tap-spec",
    "coverage": "babel-node ./node_modules/.bin/babel-istanbul cover test/test.*",
    "start": "npm run build && open index.html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-istanbul": "^0.6.1",
    "react-addons-test-utils": "^0.14.7",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0",
    "tape-run": "^2.1.3",
    "watchify": "^3.7.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
