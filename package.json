{
  "name": "testing-react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "npm run clean:lib && npm run clean:test",
    "clean:lib": "rm -rf ./lib && mkdir lib",
    "clean:test": "rm -rf ./test/lib && mkdir ./test/lib",
    "build": "npm run build:lib && npm run build:test",
    "build:lib": "npm run clean:lib && babel src -d lib",
    "build:test": "npm run clean:test && babel ./test -d ./test/lib",
    "test": "npm run build && node test/lib/test.* | faucet",
    "coverage": "npm run build && istanbul cover test/lib/test.*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "react": "^0.14.7"
  },
  "devDependencies": {
    "istanbul": "^0.4.2",
    "react-addons-test-utils": "^0.14.7",
    "tape": "^4.4.0",
    "tape-run": "^2.1.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
